{
  "name": "nft-metadata",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "ts-json-schema-generator": "^0.95.0"
  },
  "scripts": {
    "draft-1:build:schema": "cd schema-draft-1 && ts-json-schema-generator --path ./nft-metadata.ts --additional-properties true -o ./nft-metadata-schema.json --type 'NftMetadataMediaTypes'",
    "draft-1:build:docs": "cd schema-draft-1 && generate-schema-doc ./nft-metadata-schema.json ./docs/index.html"
  }
}
